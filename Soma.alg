algoritmo "Soma"
// Função :
// Autor :
// Data : 10/02/2023
// Seção de Declarações 
var
   Numero1, Numero2, Soma: Inteiro

inicio
      Escreva ("informe um número: ")
      Leia(Numero1)
      Escreva ("informe outro número: ")
      Leia(Numero2)
      Soma <- Numero1 + Numero2
      Escreva ("A soma é ", Soma)
fimalgoritmo
