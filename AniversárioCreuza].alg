algoritmo "AniversárioCreuza"

var
     ano_nasc, ano_atual, idade: inteiro
inicio
      Escreva ("em que ano nós estamos? ")
      leia (ano_atual)
      Escreva ("em que ano você nasceu? ")
      Leia (ano_nasc)
      Idade <- ano_atual - ano_nasc
      Escreva ("minha idade é ",idade, " anos")
      

fimalgoritmo
